{
  "extends": [
    "../../tslint.json",
    "tslint-react"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "jsx-boolean-value": [
      true,
      "never"
    ],
    "jsx-alignment": false,
    "jsx-no-multiline-js": false,
    "jsx-curly-spacing": false,
    "jsx-no-lambda": false
  }
}